#import "BaseCardType.h"

// 联想牌型
@interface AssociatedCardType : BaseCardType

/**
 *  返回联想牌型（从一个数组中去找牌型）
 *
 *  @param handsCards    用户手牌
 *  @param selectedCards 已经选择的牌
 *  @param otherCards    上一家出的牌
 *
 *  @return 可能的牌型->选好的牌
 */
- (NSMutableArray *)gotAssociatedCardsByHandsCards:(NSArray *)handsCards
                                     selectedCards:(NSArray *)selectedCards
                                        otherCards:(NSArray *)otherCards;

@end
